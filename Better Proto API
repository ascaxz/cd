x=game:HttpGet("https://raw.githubusercontent.com/J-61042/cd/master/Self Updater")
h = function()
	getgenv().isfile = function(File)
		local work,err = pcall(readfile, File) --Proto will error if the file you try to read does not exist. Making this easy for me to do a pcall check on.
		if not work then
			return false
		else
			return true	
		end
	end
	
	getgenv().test = function()
		return nil
	end
	
	getgenv().is_cd_caller = function()
		return true
	end
	
	
end
if x:sub(x,1,x:len()) ~= "1" then
	if ask_prompt("cd/","Hey, there is an update, would you like to get it?","Yes","No") == 1 then
		writefile("Better Proto API.lua",game:HttpGet("https://raw.githubusercontent.com/J-61042/cd/master/Better Proto API"))
		dofile("Better Proto API.lua")
	else
		h()
	end
else
	h()
end
